    },
    "homepage": "https://github.com/georgernstgraf/Uhrentracker#readme",
    "dependencies": {
        "@prisma/client": "^5.13.0",
        "connect-flash": "^0.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "ejs": "^3.1.9",
        "express": "^4.19.2",
        "express-flash": "^0.0.2",
        "express-jwt": "^8.4.1",
        "express-session": "^1.18.0",
        "express-static": "^1.2.6",
