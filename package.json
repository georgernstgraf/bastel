{
    "name": "watchtracker",
    "version": "2.0.0",
    "description": "Tracker for wristwatch inaccuracies",
    "main": "main.js",
    "scripts": {
        "start": "node bin/app.js",
        "install-htmx": "rsync -vt node_modules/htmx.org/dist/htmx*js* static/",
